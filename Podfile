# platform :ios, '9.0'

target 'test_ui' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  
  pod 'SnapKit', '~> 4.0'
  pod 'SDWebImage', '~> 5.0'
  # Pods for test_ui

  target 'test_uiTests' do
    inherit! :search_paths
		pod 'SDWebImage', '~> 5.0'
		pod 'SnapKit', '~> 4.0'
		
    # Pods for testing
  end

  target 'test_uiUITests' do
    inherit! :search_paths
		pod 'SDWebImage', '~> 5.0'
		pod 'SnapKit', '~> 4.0'
		# Pods for testing
  end

end
